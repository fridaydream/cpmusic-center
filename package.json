{
  "private": true,
  "scripts": {
    "build": "cross-env umi build",
    "start": "node server/index.js",
    "dev": "cross-env UMI_UI=none NODE_ENV=development concurrently \"umi dev\" \"nodemon server.js\"",
    "client:dev": "cross-env UMI_UI=none NODE_ENV=development umi dev",
    "server:dev": "cross-env UMI_UI=none NODE_ENV=development nodemon server.js",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@ant-design/pro-form": "^1.13.10",
    "@ant-design/pro-layout": "^6.10.9",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@babel/runtime-corejs2": "^7.12.5",
    "@umijs/preset-react": "^1.5.17",
    "@umijs/test": "^3.2.9",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^5.0.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "koa-session": "^6.1.0",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.20",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.12",
    "ramda": "^0.27.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.9"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "got": "^11.5.0",
    "http-proxy-middleware": "^1.0.5",
    "koa": "^2.13.0",
    "koa-compress": "^5.0.1",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "koa-websocket": "^6.0.0",
    "koa2-proxy-middleware": "^0.0.4",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
